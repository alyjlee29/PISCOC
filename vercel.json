{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@2",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}