{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@2",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/public/$1"
        }
    ],
    "headers": [
        {
            "source": "/dist/public/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}